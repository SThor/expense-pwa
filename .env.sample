VITE_FIREBASE_AUTH_DOMAIN=settle-up-sandbox.firebaseapp.com # SettleUp Sandbox Auth Domain
VITE_FIREBASE_DATABASE_URL=https://settle-up-sandbox.firebaseio.com # SettleUp Sandbox Database URL
VITE_SETTLEUP_GROUP_NAME="your group name here" # SettleUp Test Group Name

# VPS Deployment Configuration
VPS_HOST=your-vps-ip-or-domain.com # VPS Host IP or Domain, without www prefix or http(s)://
VPS_USERNAME=github-deploy # VPS Username for deployment
VPS_DOMAIN=your-domain.com # Domain for the deployed application, without www prefix or http(s)://
VPS_EMAIL=your-email@example.com # Email for Let's Encrypt SSL certificate registration